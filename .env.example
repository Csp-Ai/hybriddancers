# Example environment configuration for Hybrid Dancers
# Rename to `.env` and fill in your credentials. These values are loaded at
# build time and during local development. Do NOT commit real secrets.

# Stripe API keys for payments
STRIPE_SECRET_KEY=sk_test_your_secret_key
STRIPE_PUBLIC_KEY=pk_test_your_public_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key
STRIPE_PRICE_ID=price_test_id

# Base URL for local server
DOMAIN_URL=http://localhost:4242

# Supabase configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
OPENAI_API_KEY=sk-openai
# Instagram Basic Display token used by `/api/fetchOEmbed`.
# Add this variable to your Vercel project environment as well.
IG_OEMBED_TOKEN=your_instagram_oembed_token

# Optional server port
PORT=4242

# Email settings for Nodemailer
GMAIL_USER=your_gmail_address
GMAIL_PASS=your_gmail_app_password
SENDGRID_API_KEY=your_sendgrid_key
# Default address used by the site for outgoing mail
EMAIL_FROM=no-reply@hybriddancers.com
ADMIN_EMAIL=admin@example.com
